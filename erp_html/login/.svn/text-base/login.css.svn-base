.login_button {
	text-align:center;
	width:80px;
	height:26px;
	color: white;
	border: 1px solid rgb(0, 136, 204);
	line-height: 24px;
	vertical-align:middle;
	font-family:Microsoft Yahei;
	box-shadow: 0px 1px 0px rgba(255, 255, 255, 0.2) inset;
	background: -moz-linear-gradient(center top, rgb(63, 159, 229) 5%,
		rgb(19, 96, 188) 100%) repeat scroll 0% 0% transparent;
	background-color: #08c;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	-o-border-radius: 2px;
	border-radius:2px;
	line-height: 20px;
	-webkit-box-shadow: 0 1px 0 rgba(255, 255, 255, 0.2) inset;
	-moz-box-shadow: 0 1px 0 rgba(255, 255, 255, 0.2) inset;
	box-shadow: 0 1px 0 rgba(255, 255, 255, 0.2) inset;
	background: #3f9fe5;
	background: -moz-linear-gradient(top, #3f9fe5 5%, #1360bc 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(5%, #3f9fe5),
		color-stop(100%, #1360bc));
	background: -webkit-linear-gradient(top, #3f9fe5 5%, #1360bc 100%);
	background: -o-linear-gradient(top, #3f9fe5 5%, #1360bc 100%);
	background: -ms-linear-gradient(top, #3f9fe5 5%, #1360bc 100%);
	background: linear-gradient(top, #3f9fe5 5%, #1360bc 100%);
	filter: progid:DXImageTransform.Microsoft.gradient(  startColorstr='#3f9fe5',
		endColorstr='#1360bc', GradientType=0);
	cursor: pointer;
}
.login_button:hover{
	background: #3fafff;
	background: -moz-linear-gradient(top, #3faff5 5%, #1360bc 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(5%, #3faff5),
		color-stop(100%, #1360bc));
	background: -webkit-linear-gradient(top, #3faff5 5%, #1360bc 100%);
	background: -o-linear-gradient(top, #3faff5 5%, #1360bc 100%);
	background: -ms-linear-gradient(top, #3faff5 5%, #1360bc 100%);
	background: linear-gradient(top, #3faff5 5%, #1360bc 100%);
}
.login_button:ACTIVE{
	background: #2e8ed4;
	background: -moz-linear-gradient(top, #2e8ed4 5%, #1360bc 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(5%, #2e8ed4),
		color-stop(100%, #1360bc));
	background: -webkit-linear-gradient(top, #2e8ed4 5%, #1360bc 100%);
	background: -o-linear-gradient(top, #2e8ed4 5%, #1360bc 100%);
	background: -ms-linear-gradient(top, #2e8ed4 5%, #1360bc 100%);
	background: linear-gradient(top, #2e8ed4 5%, #1360bc 100%);
}

